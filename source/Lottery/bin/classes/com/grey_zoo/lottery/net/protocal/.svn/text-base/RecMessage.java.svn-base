package com.grey_zoo.lottery.net.protocal;

public class RecMessage {

	//返回的头
	private RecHead head;
	//返回的消息体
	private RecBody body;

	private String str_body;
	
	public RecHead getHead() {
		return head;
	}

	public void setHead(RecHead head) {
		this.head = head;
	}

	public RecBody getBody() {
		return body;
	}

	public void setBody(RecBody body) {
		this.body = body;
	}

	public String getStr_body() {
		return str_body;
	}

	public void setStr_body(String str_body) {
		this.str_body = str_body;
	}
	
}
